<div class="container">

    <div class="row">
        {{#each news}}
        <div class="col s12 m6">
            <div class="card-panel pink lighten-2">
                <span class="white-text">{{this}}</span>
            </div>
        </div>
        {{/each}}
    </div>

    <p class="range-field">
        <input type="range" id="range" min="1" max="10" value="{{news.length}}" onchange="getNews()"/>
    </p>

</div>